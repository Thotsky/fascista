			Authoritarian_Socialism = {
	
		types = {
			Authoritarian_Socialism
		}
		
		dynamic_faction_names = {
		}
		
		color = { 155 59 59 }
		
		rules = {
			can_declare_war_on_same_ideology = yes
			can_force_government = yes
			can_send_volunteers = yes
			can_puppet = yes
			can_lower_tension = yes
			can_only_justify_war_on_threat_country = no
			can_guarantee_other_ideologies = no
		}

                can_host_government_in_exile = no
		
		war_impact_on_world_tension = 0.2
		faction_impact_on_world_tension = 0.2
		
		modifiers = {
			generate_wargoal_tension = 0
			join_faction_tension = 0
			lend_lease_tension = 0
			send_volunteers_tension = 0
			guarantee_tension = 0
			take_states_cost_factor = -0.1
			annex_cost_factor = -0.1
			justify_war_goal_when_in_major_war_time = -0.5
			drift_defence_factor = 0.2
			puppet_cost_factor = -0.25
		}

		can_be_boosted = yes
		can_collaborate = yes
		
		ai_<ideology> = yes
	}
